<%= turbo_frame_tag "admin_nav", target: "_top" do %>
  <%= render "header" do %>
    <% if logged_in? %>
      <span class="nav__item">
        <%= link_to t('application.admin'), admin_path, class: 'nav__link' %>
      </span>
      <span class="nav__item">
        <%= link_to t('sessions.sessions'), user_sessions_path, class: 'nav__link' %>
      </span>
      <span class="nav__item">
        <%= link_to t('application.good_job'), good_job_path, class: 'nav__link' %>
      </span>
      <span class="nav__item">
        <%= link_to t('sessions.logout'), logout_path, class: 'nav__link' %>
      </span>
    <% end %>
  <% end %>
<% end %>
