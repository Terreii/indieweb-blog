<tr>
  <td><%= link_to entry.title, entry.entryable %></td>
  <td><%= entry.entryable_type %></td>
  <td><%= entry.published_at || t("admin.draft") %></td>
  <td>
    <span class="actions_cell">
      <%= link_to "Edit", [:edit, entry.entryable] %> |
      <%= button_to "Delete", entry.entryable, method: :delete, class: "btn btn-danger", data: {
        turbo_confirm: t("admin.delete_check")
      } %>
    </span>
  </td>
</tr>
